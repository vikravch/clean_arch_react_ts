{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb-typescript",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react", "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        // your rules configuration here
        "@typescript-eslint/dot-notation": "error"
    }
}
